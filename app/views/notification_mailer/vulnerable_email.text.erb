    <%= log.server.display_name %> (<%=  link_to log.server.display_name, server_app_url(log.bundle, server_id: log.server.id) %>)
    <% else %>
    Monitor:
    <%= log.bundle.display_name %> (<%= link_to log.bundle.display_name, monitors_url(log.bundle) %>)
    <% end %>
    
    Package: 
    <%= log.package_name -%>
    
    Upgrade to: 
    <%= log.upgrade_to.first -%>


  <%  end %>

